{
  "name": "prompt-helper",
  "icon": "docs/images/icon.jpg",
  "displayName": "Prompt-Helper for front-end snippets",
  "description": "This is a VS Code plugin for quickly generating JSDoc-style comments. Used for common front-end structured comments to facilitate triggering copilot",
  "version": "1.0.2",
  "engines": {
    "vscode": "^1.80.0"
  },
  "publisher": "TomaLau",
  "repository": {
    "type": "git",
    "url": "https://github.com/JerAxai/prompt-engineering-snippets.git"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "path": "./snippets/tool.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/fn.code-snippets"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/fn.code-snippets"
      },
      {
        "language": "typescript",
        "path": "./snippets/fn.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/fn.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/react.code-snippets"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/react.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/vue.code-snippets"
      }
    ]
  }
}
